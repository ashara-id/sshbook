{
  "name": "sshbook",
  "version": "1.0.0",
  "description": "Simple SSH config reader",
  "main": "index.js",
  "dependencies": {
    "cross-spawn": "^7.0.2",
    "electron-settings": "^3.2.0",
    "lodash": "^4.17.15",
    "ssh-config": "^4.0.2"
  },
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-packager": "^14.2.1"
  },
  "scripts": {
    "start": "electron . --dev",
    "build-osx": "electron-packager . 'SSH Book' --platform darwin --icon ./assets/icons/sshbook.icns --out ./dist --overwrite --extend-info ./assets/Info.plist --asar --prune"
  },
